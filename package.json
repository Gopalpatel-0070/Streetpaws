{
  "name": "streetpaws-app",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm --prefix frontend run dev",
    "dev:backend": "npm --prefix backend run dev",
    "start:backend": "npm --prefix backend run start",
    "start:frontend": "npm --prefix frontend run dev",
    "start:all": "concurrently \"npm --prefix backend run start\" \"npm --prefix frontend run dev\"",
    "server": "npm run start:backend",
    "build": "npm --prefix frontend run build",
    "preview": "npm --prefix frontend run preview"
  },
  "workspaces": [
    "frontend",
    "backend"
  ],
  "dependencies": {},
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
